{
  "cSpell.language": "en-GB",
  "cSpell.enabled": true,
  "cSpell.enableFiletypes": [
    "shellscript"
  ],
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "challenge/",
    "!challenge/info.txt"
  ],
  "cSpell.ignoreWords": [
    "ende",
    "picoctf",
    "venv"
  ]
}
